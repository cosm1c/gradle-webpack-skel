{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "strict": true,
    "moduleResolution": "node",
    "alwaysStrict": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "checkJs": true,
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "jsx": "react",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "removeComments": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "outDir": "frontend/out",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
    "lib": [
      "dom",
      "es6",
      "es2017.object"
    ]
  },
  "exclude": [
    "../node_modules",
    "gulpfile.js",
    "webpack.dev.config.js",
    "webpack.prd.config.js",
    "webpack.prd-BACKUP.config.js",
    "dist"
  ]
}
