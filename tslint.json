{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ]
  },
  "rules": {
    "interface-name": false,
    "max-line-length": false,
    "no-empty-interface": false,
    "no-console": false,
    "no-reference": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ]
  },
  "rulesDirectory": []
}
